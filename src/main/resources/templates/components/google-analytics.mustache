<script async src="https://www.googletagmanager.com/gtag/js?id={{userId}}"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', '{{userId}}');
</script>
